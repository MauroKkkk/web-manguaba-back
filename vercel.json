{
    "version": 2,
    "rewritres": [{ "source": "/(.*)", "destination": "/"}],
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "server.js"
        },
        {
            "src": "/Arquivos/(.*)",
            "dest": "/views/index.ejs"
        }
    ]
}